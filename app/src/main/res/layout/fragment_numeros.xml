<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.NumerosFragment">

    <ImageView
        android:id="@+id/imageUm"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/um"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageDois"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="49dp"
        android:src="@drawable/dois"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageTres"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="43dp"
        android:src="@drawable/tres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageUm" />

    <ImageView
        android:id="@+id/imageQuatro"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="52dp"
        android:src="@drawable/quatro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageDois" />

    <ImageView
        android:id="@+id/imageCinco"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/cinco"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageTres" />

    <ImageView
        android:id="@+id/imageSeis"
        android:layout_width="123dp"
        android:layout_height="135dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="52dp"
        android:src="@drawable/seis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageTres" />
</androidx.constraintlayout.widget.ConstraintLayout>